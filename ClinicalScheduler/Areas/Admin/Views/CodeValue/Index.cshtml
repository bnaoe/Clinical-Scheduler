@{
    ViewData["Title"] = "Index";
}

<div class="container p-3 border">
    <div class="row pt-4 border">
        <div class="col-6">
            <h2 class="primary">Code Value List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller ="CodeValue" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> &nbsp; Create New
            </a> 
        </div>
    </div>


<table id ="tblData" class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>Code Value No</th>
            <th>Code Set</th>
            <th>Name</th>
            <th>Description</th>
            <th></th>
        </tr>
    </thead>
    
</table>
</div>

@section Scripts{
    <script src="~/js/CodeValue.js"></script>
}


