
@{
    ViewData["Title"] = "Index";
}

<div class="container mt-4 p-3 border">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="primary">User List</h2>
        </div>
        <div class="col-6 text-end">
           <a class="btn btn-primary" asp-area="Identity" asp-page="/Account/Register">
               <i class="bi bi-plus-circle"></i> &nbsp; Create New User</a>
        </div>
    </div>


    <table id ="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Middle Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Location</th>
                <th>Role</th>
                <th></th>
            </tr>
        </thead>
   
    </table>
</div>
@section Scripts{
    <script src="~/js/ApplicationUser.js"></script>
}
